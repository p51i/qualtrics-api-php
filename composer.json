{
  "name": "p51i/qualtrics-api-php",
  "version": "1.0.0-alpha",
  "type": "library",
  "description": "PHP library for v3 of the Qualtrics API",
  "keywords": ["qualtrics", "survey"],
  "homepage": "https://github.com/p51i/qualtrics-api-php",
  "require": {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": "^6.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.21"
  },
  "autoload": {
    "psr-4": {
      "Qualtrics\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qualtrics\\Tests\\": "tests/src/"
    }
  }
}
